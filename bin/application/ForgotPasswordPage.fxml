<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ForgotPasswordViewController">
	<!-- TODO Add Nodes -->
	<children>
      <TextField fx:id="usernameField" layoutX="225.0" layoutY="122.0" />
      <Label fx:id="userNameLabel" layoutX="226.0" layoutY="105.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="228.0" layoutY="155.0" prefHeight="17.0" prefWidth="54.0" text="Reset Key" />
      <PasswordField fx:id="resetKeyField" layoutX="227.0" layoutY="172.0" />
      <Button fx:id="submit" layoutX="226.0" layoutY="270.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="25.0" prefWidth="149.0" style="-fx-background-color: black;" text="Reset" textFill="WHITE" />
      <Label fx:id="errorLabel" layoutX="286.0" layoutY="64.0" style="-fx-background-color: red;" />
      <Label fx:id="ForgotPasswordPageLabel" layoutX="228.0" layoutY="58.0" text="Forgot Password">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <Button fx:id="backButton" layoutX="31.0" layoutY="57.0" mnemonicParsing="false" onAction="#backToLogin" style="-fx-background-color: transparent; -fx-border-color: black;" text="&lt;">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Label fx:id="wrongForgotUser" layoutX="159.0" layoutY="262.0" />
      <Label fx:id="passwordLabel1" layoutX="228.0" layoutY="207.0" prefHeight="17.0" prefWidth="79.0" text="New Password" />
      <PasswordField fx:id="newPasswordField" layoutX="227.0" layoutY="224.0" />
   </children>
</AnchorPane>
